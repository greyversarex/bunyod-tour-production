{
  "name": "workspace",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "prod:setup": "npm install && npx prisma generate && npm run build",
    "prod:migrate": "NODE_ENV=production npx prisma migrate deploy",
    "prod:start": "NODE_ENV=production pm2 start index.js --name bunyod-tour",
    "prod:restart": "pm2 restart bunyod-tour",
    "prod:stop": "pm2 stop bunyod-tour",
    "prod:logs": "pm2 logs bunyod-tour",
    "prod:status": "pm2 status"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.0",
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@uppy/aws-s3": "^5.0.0",
    "@uppy/core": "^5.0.1",
    "@uppy/dashboard": "^5.0.1",
    "@uppy/react": "^5.0.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "file-type": "^16.5.4",
    "flatpickr": "^4.6.13",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "openai": "^5.16.0",
    "prisma": "^6.15.0",
    "puppeteer": "^24.26.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
